form.col-xs-12.col-md-6.col-md-offset-3(role="form")
  include ../mixins/alert.jade
  .form-group
    input.form-control.bottom-space(type="email", name="email", placeholder="email address")
  .form-group
    input.form-control(type="password", name="password", placeholder="password")
  .form-group.form-buttons
    button.btn.btn-primary.btn-lg.active(name="submit", type="button") submit